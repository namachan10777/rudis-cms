import * as rudis from "../rudis.ts";

{%- for field in fields %}
  {%- if field.kind === "markdown" %}
export const {{ field.upperCamelCase }} =
    {%- for keep in field.keeps %}
      {%- if keep.name == "Image" %}
  | rudis.Image<{{ keep.storage }}>
      {%- else %}
  | rudis.{{ keep.name }}
      {%- endif %}
    {%- endfor %}
  {%- endif %}
{%- endfor %}
